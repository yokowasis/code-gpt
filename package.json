{
  "name": "code-gpt",
  "displayName": "code-gpt",
  "description": "VSCode ChatGPT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "Code GPT",
      "properties": {
        "codegpt.openAIKey": {
          "title": "Open AI Key",
          "type": "string"
        }
      }
    },
    "commands": [
      {
        "command": "code-gpt.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "code-gpt.askGpt",
        "title": "Ask GPT"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.80.0",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "openai": "^3.3.0"
  }
}